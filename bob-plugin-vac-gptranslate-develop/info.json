{
    "identifier": "me.vacuity.chat.gptranslate",
    "version": "2.0.1",
    "category": "translate",
    "name": "GPTranslate",
    "summary": "调用 openai-chatgpt/google-gemini/anthropic-claude/xai-grok/deepseek/ali-qwen 实现翻译功能，支持流式输出，针对思考模型支持输出思考过程",
    "icon": "",
    "author": "vacuity",
    "homepage": "",
    "appcast": "https://raw.githubusercontent.com/vacuityv/bob-plugin-vac-gptranslate/develop/appcast.json",
    "minBobVersion": "0.5.0",
    "options": [
        {
            "identifier": "server",
            "type": "menu",
            "title": "服务器",
            "defaultValue": "china",
            "menuValues": [
                {
                    "title": "国内-切换后需重启",
                    "value": "china"
                },
                {
                    "title": "美国-切换后需重启",
                    "value": "usa"
                }
            ]
        },
        {
            "identifier": "loginAccount",
            "type": "text",
            "title": "登录邮箱"
        },
        {
            "identifier": "loginPassword",
            "type": "text",
            "title": "登录密码"
        },
        {
            "identifier": "modelType",
            "type": "menu",
            "title": "模型选择",
            "defaultValue": "gpt-4o",
            "menuValues": [
                {
                    "title": "gpt-4.1",
                    "value": "gpt-4.1"
                },
                {
                    "title": "gpt-4o",
                    "value": "gpt-4o"
                },
                {
                    "title": "o4-mini",
                    "value": "o4-mini"
                },
                {
                    "title": "o3",
                    "value": "o3"
                },
                {
                    "title": "gemini-2.0-flash-exp",
                    "value": "gemini-2.0-flash-exp"
                },
                {
                    "title": "gemini-2.5-pro",
                    "value": "gemini-2.5-pro"
                },
                {
                    "title": "claude-sonnet-4",
                    "value": "claude-sonnet-4"
                },
                {
                    "title": "claude-3.7-sonnet",
                    "value": "claude-3.7-sonnet"
                },
                {
                    "title": "claude-3.5-sonnet",
                    "value": "claude-3.5-sonnet"
                },
                {
                    "title": "grok-3",
                    "value": "grok-3"
                },
                {
                    "title": "deepseek-v3",
                    "value": "deepseek-chat"
                },
                {
                    "title": "deepseek-r1",
                    "value": "deepseek-reasoner"
                },
                {
                    "title": "ali-qwen-plus",
                    "value": "qwen-plus"
                }
            ]
        },
        {
            "identifier": "useStreamFlag",
            "type": "menu",
            "title": "流式传输",
            "defaultValue": "y",
            "menuValues": [
                {
                    "title": "不使用",
                    "value": "n"
                },
                {
                    "title": "使用(需要版本>=1.8.0)",
                    "value": "y"
                }
            ]
        },
        {
            "identifier": "showThoughtFlag",
            "type": "menu",
            "title": "显示思考过程",
            "defaultValue": "y",
            "menuValues": [
                {
                    "title": "是(仅针对思考模型，比如deepseek-r1/claude-3.7-sonnet)",
                    "value": "y"
                },
                {
                    "title": "否",
                    "value": "n"
                }
            ]
        },
        {
            "identifier": "prompt",
            "type": "text",
            "title": "prompt",
            "textConfig": {
                "type": "visible",
                "height": 60,
                "placeholderText": "请输入您自定义的prompt（如果你不知道prompt是什么，请不要输入）"
            }
        }
    ]
}
